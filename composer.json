{
    "name": "hubspot/api-client",
    "version": "8.3.0",
    "description": "Hubspot API client",
    "keywords": [
        "hubspot",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://hubspot.com/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "HubSpot",
            "homepage": "https://hubspot.com/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.5",
        "phpspec/phpspec": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "HubSpot\\Client\\" : "codegen/",
            "HubSpot\\" : "lib/",
            "Hubspot\\Tests\\": "tests/"
        }
    }
}
